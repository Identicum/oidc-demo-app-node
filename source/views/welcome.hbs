<h2>Claims</h2>
<table class="table table-hover">
	<thead>
		<tr style="color: #00b4c5;">
			<th>Name</th>
			<th>Value</th>
		</tr>
	</thead>
	<tbody>
	{{#each claims}}
		<tr class="idtoken-claim">
			<td>{{@key}}</td>
			<td>{{this}}</td>
		</tr>
	{{/each}}
	</tbody>
</table>

<h2>id_token - JSON claims</h2>
<code class="idtoken-json">
	{{ json claims }}
</code>

<h2>id_token - JSON token</h2>
<code class="idtoken-json">
	{{ json token }}
</code>

<h2>Userinfo</h2>
<table class="table table-hover">
	<thead>
		<tr style="color: #00b4c5;">
			<th>Name</th>
			<th>Value</th>
		</tr>
	</thead>
	<tbody>
	{{#each userinfo}}
		<tr class="idtoken-claim">
			<td>{{@key}}</td>
			<td>{{this}}</td>
		</tr>
	{{/each}}
	</tbody>
</table>
